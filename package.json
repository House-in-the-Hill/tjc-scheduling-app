{
    "name": "tjc-scheduling-app",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "server": "nodemon --exec babel-node ./server/index.js",
        "seed": "node script/seed.js",
        "prettier:base": "prettier",
