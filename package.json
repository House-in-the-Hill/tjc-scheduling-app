        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/register": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "babel-loader": "^8.1.0",
        "babel-preset-expo": "~8.0.0",
        "body-parser": "^1.19.0",
        "chalk": "^4.0.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
