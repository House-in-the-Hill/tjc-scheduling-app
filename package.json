    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./src/*.{js,jsx,ts,tsx}": [
            "npm run prettier:write",
            "eslint src/*.js --fix-dry-run"
        ]
    },
    "dependencies": {
