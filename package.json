        "@react-navigation/stack": "^5.2.9",
        "axios": "^0.19.2",
        "crypto": "^1.0.1",
        "dotenv": "^8.2.0",
        "expo": "~36.0.0",
        "expo-font": "~8.0.0",
        "express": "^4.17.1",
        "pg": "^8.0.2",
        "pg-hstore": "^2.3.3",
        "react": "~16.9.0",
        "react-dom": "~16.9.0",
